<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="isabelle.eclipse.product.feature"
      label="%featureName"
      version="1.2.0.qualifier"
      provider-name="%providerName">

   <description url="%descriptionURL">
      %description
   </description>

   <copyright url="%copyrightURL">
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <url>
      <update label="%updateSiteName" url="http://andriusvelykis.github.com/isabelle-eclipse/updates/isabelle2013/nightly/"/>
   </url>

   <requires>
      <import plugin="org.eclipse.core.runtime" version="3.2.0" match="compatible"/>
      <import plugin="org.eclipse.ui" version="3.3.0" match="compatible"/>
      <import plugin="com.ibm.icu"/>
      <import feature="org.eclipse.platform"/>
      <import feature="in.iitd.mldev.feature"/>
   </requires>

   <plugin
         id="isabelle.eclipse"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.ui.views.log"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
